{
  "name": "@mindfiredigital/pivot-head-core",
  "version": "1.0.0",
  "main": "./dist/pivothead-core.umd.js",
  "module": "./dist/pivothead-core.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mindfiredigital/PivotHead/"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "pivothead-core",
    "pivot",
    "pivotTablepivot",
    "table",
    "spreadsheet",
    "javascript",
    "typescript",
    "data-drill-down",
    "data-analysis",
    "data-visualization",
    "dynamic-table",
    "data-aggregation",
    "data-grid",
    "interactive-reports",
    "data-exploration",
    "olap",
    "data-pivoting",
    "data-summarization",
    "react-pivot",
    "business-intelligence",
    "analytics-tool"
  ],
  "scripts": {
    "build": "pnpm run test && vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint \"src/**/*.ts*\"",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.32.0"
  },
  "release": {
    "repositoryUrl": "https://github.com/mindfiredigital/PivotHead.git",
    "branches": [
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  }
}
